<div class="pageheader1">
  <h1>Listing enzymes</h1>
</div>
<div class="main-content">
  <table class="standard">
    <tr>
      <th>Name</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>

    <% @enzymes.each do |enzyme| %>
    <tr>
      <td><%= enzyme.name %></td>
      <td><%= link_to 'Show', enzyme %></td>
      <td><%= link_to 'Edit', edit_enzyme_path(enzyme) %></td>
      <td><%= link_to 'Destroy', enzyme, confirm: 'Are you sure?', method: :delete %></td>
    </tr>
    <% end %>
  </table>

  <br />

  <%= link_to 'New Enzyme', new_enzyme_path %>
</div>
