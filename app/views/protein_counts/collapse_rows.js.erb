var el = $(<%= row_id(@parent_taxon)%>);
<% @taxons.each do |taxon| %>
  var row = $("#<%=row_id(taxon)%>");
  row.remove();
<% end %>
el.children('.taxon').html("<%= (j link_to @parent_taxon.name, { :controller => "protein_counts", :action => "add_row", :parent_id => @parent_taxon.id, :level => @level}, :remote => true)%>");


