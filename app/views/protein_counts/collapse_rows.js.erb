var el = $(<%= row_id(@parent_taxon)%>);
<% if @after_children_taxon %> 
el.nextUntil(<%= row_id(@after_children_taxon) %>,"tr").remove();
<% else %>
el.nextAll("tr").remove();
<% end %>


el.children('.taxon').find('.sign').html("<%= (j link_to "+", { :controller => "protein_counts", :action => "add_row", :parent_id => @parent_taxon.id, :level => @level}, :remote => true)%>");


