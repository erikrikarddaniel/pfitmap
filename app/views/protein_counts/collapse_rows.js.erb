var el = $(<%= row_id(@parent_taxon)%>);
<% if @after_children_taxon %> 
el.nextUntil(<%= row_id(@after_children_taxon) %>,"tr").remove();
<% else %>
el.nextAll("tr").remove();
<% end %>

el.children('.taxon').html("<%= (j link_to_expand("+", @parent_taxon, @enzyme_tree.keys, @level)) %><%= @parent_taxon.name %>");


