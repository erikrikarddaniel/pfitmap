<%= form_for(@profile) do |f| %>
<%= render 'shared/error_messages' %>

<%= f.label :name %>
<%= f.text_field :name %>

<%= f.label :parent_profile_id, "Parent Profile Id (leave blank for no parent)" %>
<%= f.number_field :parent_profile_id %>

<%= f.submit "Create Profile", :class => "btn btn-large btn-primary"%>

<% end %>
