<%= form_for(@result) do |f| %>
<%= render 'shared/error_messages_result' %>

<%= f.label :profile_id, "HMM Profile" %>
<%= f.collection_select(:profile, @profiles, :id, :name) %>

<%= f.label :file, "Choose a file containing your results" %>
<%= f.file_field :file %>

<%= f.submit "Create Sequence", :class => "btn btn-large btn-primary"%>

<% end %>
